   "source": [
    "from glob import glob\n",
    "from PyPDF2 import PdfMerger"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "folders = glob(\"./I.BA_RL_MM.H2201/*\")\n",
    "files = []\n",
    "for folder in folders:\n",
    "    files += glob(folder + \"/*.pdf\")\n",
    "merger = PdfMerger()\n",
    "for pdf in files:\n",
    "    merger.append(pdf)\n",
    "merger.write(\"RL.pdf\")\n",
    "merger.close()"
   ]
  },
